<% for(var i=0;i<users.length;i++){%>
    <tr>
        <td>
            <%=i+1%>
        </td>
        <td>
            <%=users[i].name%>
        </td>
        <td>
            <%=users[i].quantity%>
        </td>
        <td>
            <%=users[i].price%>
        </td>
        <td>
            <%=users[i].category%>
        </td>
        <td>
            <a href="/update-user?id=<%=users[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fa-solid fa-wrench fa-beat" style="color: #ffffff;"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%=users[i]._id %>>
                <span class="text-gradient "><i class="fa-solid fa-trash fa-beat" style="color: #ffffff;"></i></span>
            </a>
        </td>
    </tr>
    <% } %>